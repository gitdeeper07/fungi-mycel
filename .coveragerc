[run]
source = fungi_mycel
omit = 
    tests/*
    setup.py
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    if 0:
    if False:
show_missing = True
precision = 2

[html]
directory = coverage_html
